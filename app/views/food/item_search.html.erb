<div>
  Can't find what you're looking for?
  <%= button_to "Add Food Item", food_new_path, class: 'btn btn-primary', :method => :get %>
</div>

<h3> Results </h3>
<% if @results.empty? %>
  No results found.
<% end %>
<ul>
<% @results.each do |food| %>
 <li><%= link_to food.name, "#add-food-item", :data => {:toggle => "modal", :href => food_add_through_modal_path(:id => food.id)}, :class => "myModal" %></li>
<% end %>
</ul>

<div class="modal fade" id="add-food-item" tabindex="-1" role="dialog"aria-labelledby="addItemLabel" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
        <h4 class="modal-title" id="modalTitle"> Add Food To Meal </h4>
      </div>
      <div class="modal-body">
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
      </div>
    </div>
  </div>
</div>
